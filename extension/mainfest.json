{
   "manifest_version": 2,

   "name": "PR++",
   "description": "Link teacher names on PASS to PR++ website",
   "version": "1.4.1",
   "permissions": [
      "webRequest",
      "*://*.polyratings.com/"
    ],
    "icons": { "48": "logo48.png",
      "128": "logo128.png" },
   "content_scripts": 
   [
      {
         "matches": ["http://pass.calpoly.edu/main.html"],
         "js": ["linker.js"],
         "run_at": "document_end"
      }
   ]
}
